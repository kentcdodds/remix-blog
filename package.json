{
  "name": "remix-starter-express",
  "version": "1.0.0",
  "scripts": {
    "build": "cross-env NODE_ENV=production concurrently \"remix build\" \"tsc -b\"",
    "start": "cross-env NODE_ENV=development concurrently \"remix run\" \"tsnd --ignore app --respawn server.js\"",
    "typecheck": "tsc",
    "temp": "node ./temp.ignored.js"
  },
  "dependencies": {
    "@mdx-js/mdx": "^1.6.19",
    "@mdx-js/react": "^1.6.19",
